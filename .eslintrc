{
    "extends": ["airbnb"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "globals": {
        "document": true,
        "window": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/forbid-prop-types": 0,
        "react/no-danger": 0,
        "jsx-a11y/no-autofocus": 0
    },
    "settings": {
        "import/resolver": {
          "node": {
            "moduleDirectory": ["node_modules", "/"]
          }
        }
    }
  }